{
  "from": {
    "mainTable": "biz_task_log"
  },
  "selects": [
    {
      "column": "taskId"
    },
    {
      "column": "date"
    },
    {
      "column": "taskCode"
    }
  ],
  "conditions": [
    {
      "column": "taskId",
      "operator": "not like",
      "value":"%已完成%"
    },
    {
      "column": "devId",
      "operator": "=",
      "value": "_par.deviceId"
    },
    {
      "operator": "or",
      "conditions": [
        {
          "column": "isOver",
          "operator": "=",
          "value": "0"
        },
        {
          "column": "isOver",
          "operator": "is null"
        }
      ]
    }
  ],
  "page": 1,
  "pageSize": 1,
  "orderBy": {
    "columns": [
      {
        "column": "taskId",
        "direction": "desc"
      }
    ]
  }
}